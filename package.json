    "ir-clone": "^1.2.1",
    "ir-reattach": "^5.0.0",
    "is-promise": "^2.1.0",
    "lodash.defaults": "^4.0.1",
    "lru-cache": "^5.1.1",
    "mime-types": "^2.1.17",
    "node-fetch": "^2.0.0",
    "once": "^1.4.0",
    "steal": "^2.0.0",
    "text-encoding": "^0.6.4",
    "useragent": "^2.3.0",
    "websocket": "^1.0.22",
    "whatwg-url": "^7.0.0",
    "xmlhttprequest2": "^1.0.0"
  },
  "greenkeeper": {
    "ignore": [
      "copy-dir"
    ]
  }
